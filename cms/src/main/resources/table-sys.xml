        <table name="think_sys_user_rowfilter" comment="用户行级过滤" ds="think"
               pk="id">
            <fields>
                <field name="id" comment="主键" type="int" size="11" isnull="false"></field>
                <field name="parentid" comment="用户id" type="int" size="11"
                       isnull="false"></field>
                <field name="moduleid" comment="模块表id" type="int" size="11"
