                <field name="table" comment="数据库表名" type="varchar" size="50"></field>
                <field name="createtime" comment="创建时间" type="bigint" size="20"></field>
                <field name="data" comment="删除的数据" type="text"></field>
            </fields>
        </table>
        <table name="think_sys_cms_log" comment="CMS访问日志" ds="think_log"
