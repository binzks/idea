                <data id="3" name="模块管理" parentid="2" type="1" url="/tpl/list3"
                      viewcode="view_sys_module" status="1" icon="icon-list" order="1"
                      remark=""></data>
                <data id="4" name="角色管理" parentid="2" type="1" url="/tpl/list4"
                      viewcode="view_sys_role" status="1" icon="icon-list" order="2"
                      remark=""></data>
                <data id="5" name="系统用户" parentid="2" type="1" url="/tpl/list5"
                      viewcode="view_sys_user" status="1" icon="icon-list" order="3"
                      remark=""></data>
                <data id="6" name="个人信息" parentid="1" type="1" url="/tpl/list6"
                      viewcode="view_sys_user_info" status="1" icon="icon-list" order="1"
                      remark=""></data>
                <data id="7" name="工作流管理" parentid="2" type="1" url="/tpl/list7"
